#FUNCTION
SIF ARG > 0
	RETURNF -1
VARSET LOCAL
SELECTCASE ABL:(ARG:2):Ｃ感覚
	CASE 0
		LOCAL:0 = 1
	CASE 1
		LOCAL:0 = 20
	CASE 2
		LOCAL:0 = 400
	CASE 3
		LOCAL:0 = 5000
	CASE 4
		LOCAL:0 = 15000
	CASE 5
		LOCAL:0 = 30000
	CASE 6
		LOCAL:0 = 40000
	CASE 7
		LOCAL:0 = 50000
	CASE 8
		LOCAL:0 = 60000
	CASE 9
		LOCAL:0 = 70000
	CASEELSE
		LOCAL:0 = 20000 * (ABL:(ARG:2):Ｃ感覚 - 6)
ENDSELECT
;経験補正
LOCAL:0 = LOCAL:0 * 10 / (5 + GETEXPLV(EXP:Ｃ経験,8))
LOCAL:0 = LOCAL:0 * (1 + ABL:Ｃ感覚) / (1 + GETEXPLV(EXP:Ｃ絶頂経験,8))
;一線越えない
SIF TALENT:(ARG:2):一線越えない && !TALENT:(ARG:2):恋慕
	TIMES LOCAL:0 , 2.00
RETURNF LOCAL:(ARG:1)

