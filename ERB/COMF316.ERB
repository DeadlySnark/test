;指挿入れ
@COM316

TRYCALL COM417_is_Protected(PLAYER,TARGET,1)
SIF RESULT
	RETURN 0

;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = PLAYER

;破瓜抑制
TFLAG:2 = 1

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:体力 += 30
DOWNBASE:気力 += 80

IF !TALENT:処女
	SOURCE:快Ｖ = 120
	SOURCE:情愛 = 150
	SOURCE:苦痛 = 250
	SOURCE:欲情 = 200
	SOURCE:露出 = 120
	SOURCE:逸脱 = 150
	SOURCE:反感 = 200
;V経験が0の時はV経験を伴わない物に派生
;Vの珠は入らないが、快Vが高いと慣らしてあるため処女喪失時に影響
ELSE
	SOURCE:快Ｖ = 60
	SOURCE:情愛 = 150
	SOURCE:苦痛 = 250
	SOURCE:欲情 = 200
	SOURCE:露出 = 120
	SOURCE:逸脱 = 300
	SOURCE:反感 = 400
ENDIF
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
IF TALENT:自己愛 < 0 || TALENT:抵抗
	SOURCE:鬱屈 += 20
	;処女
	SIF TALENT:処女
		SOURCE:鬱屈 += 10
ENDIF
;Vへの苦痛
CALL PAIN_CHECK_V(TARGET)
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の膣内⇔調教者の指の汚れが移動
	STAIN:膣内 |= STAIN:PLAYER:手
	STAIN:PLAYER:手 |= STAIN:膣内
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
IF HETEROSEX(TARGET,PLAYER)
	EXP:レズ経験 += 3
	EXP:MASTER:レズ経験 += 3
ENDIF

TIME += 10

RETURN 1

