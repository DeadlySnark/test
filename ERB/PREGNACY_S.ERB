;=============================================================================
;妊娠関連のイベント
;=============================================================================
;---------------------------------------------------------
;紅魔館生活中呼び出される関数
;---------------------------------------------------------
;ARG 射精したキャラ番号, ARG:1 射精されたキャラ番号, ARG:2 射精量
;ARG == 999(998) は訪問者
@PREGNACY_S_CHECK(ARG,ARG:1,ARG:2)
;訪問者での妊娠が OFF になっている場合は除外
SIF (ARG == 998 || ARG == 999) && !GETBIT(FLAG:ＮＴＲパッチ設定,5)
	RETURN 0
	
;排卵日のみ
SIF CFLAG:(ARG:1):351 != 1
	RETURN 0
	
;住民の妊娠チェック
IF GETBIT(TALENT:(ARG:1):2,0) && TALENT:(ARG:1):妊娠 == 0 && CFLAG:(ARG:1):81 != 3 && CFLAG:(ARG:1):にんっしんっ == 0
	;妊娠判定
	CALL PREGNACY_S_NAKADASHI_CHECK(ARG,ARG:1,ARG:2)
ENDIF
RETURN 0

@PREGNACY_S_NAKADASHI_CHECK(ARG,ARG:1,ARG:2)
;妊娠するかの判定
IF RATE_PREGNANCY(ARG,ARG:1,ARG:2) > RAND:10000
	TRYCALL 妊娠確定時の追加処理, ARG:1
	CFLAG:(ARG:1):にんっしんっ = ARG + 1
ENDIF
;百発百中（デバッグ用
;TRYCALL 妊娠確定時の追加処理, ARG:1
;CFLAG:(ARG:1):にんっしんっ = ARG + 1
RETURN 0

;妊娠確率計算
@RATE_PREGNANCY(ARG, ARG:1, ARG:2)
#FUNCTION
;基本値
LOCAL = 100 * ARG:2
;素質補正
SIF TALENT:(ARG:1):幼児／幼児退行
	LOCAL /= 5
SIF TALENT:(ARG:1):体型 < 0
	LOCAL /= -1 * (TALENT:(ARG:1):体型)
SIF CFLAG:(ARG:1):81 == 2
	LOCAL = 10000
RETURNF LOCAL

;---------------------------------------------------------
;一日の終了後に呼び出される関数
;---------------------------------------------------------
@PREGNACY_S_ANNOUNCEMENT
FOR LOCAL,0,CHARANUM
	IF TALENT:LOCAL:妊娠 == 0 && CFLAG:LOCAL:にんっしんっ > 0 && LOCAL != MASTER
		PRINTFORML %CALLNAME:LOCAL%の様子がおかしい…
		PRINTFORMW %CALLNAME:LOCAL%は%N_T_R(CFLAG:LOCAL:にんっしんっ - 1, 1)%の子供を身篭った様だ
		PRINTFORMW %CALLNAME:LOCAL%は[妊娠]した
		;父親によって固有のメッセージ
		;恋慕住人、父親がMASTER
		IF TALENT:LOCAL:恋慕 && CFLAG:LOCAL:にんっしんっ -1 == 0
			PRINTFORML %CALLNAME:LOCAL%は頬を染め%CALLNAME:MASTER%の子を宿したことを伝えてきた。
		ENDIF
		;恋慕住人、父親が訪問者、誰か
		IF TALENT:LOCAL:恋慕 && CFLAG:LOCAL:にんっしんっ -1 > 900 && CFLAG:LOCAL:にんっしんっ -1 <= 1000
			PRINTFORML %CALLNAME:LOCAL%は青ざめた顔をして%CALLNAME:MASTER%に他の男の子を宿したことを伝えてきた。
			PRINTFORML 泣き崩れる%CALLNAME:LOCAL%を支えながら、%CALLNAME:MASTER%は子供に罪はないと諭すことしかできなかった。
		ENDIF
		TALENT:LOCAL:妊娠 = 1
		CALL CHANGE_N_STATUS(LOCAL)
	ELSEIF TALENT:LOCAL:妊娠 == 0 && CFLAG:LOCAL:にんっしんっ > 0 && LOCAL == MASTER && (CFLAG:MASTER:現在位置 == 900 || CFLAG:MASTER:現在位置 == 901) && CFLAG:MASTER:屈服度 < 1000
		PRINTFORML 体の調子がおかしい…
		PRINTFORMW しかしここでは検査する事もできない…
;		TALENT:LOCAL:妊娠 = 1
;		CALL CHANGE_N_STATUS(LOCAL)
	ELSEIF TALENT:LOCAL:妊娠 == 0 && CFLAG:LOCAL:にんっしんっ > 0 && LOCAL == MASTER
		PRINTFORML 体の調子がおかしい…
		PRINTFORM 永遠亭で検査してもらったところ、
		PRINTFORMW %N_T_R(CFLAG:LOCAL:にんっしんっ - 1, 1)%の子供を身篭った様だ
		PRINTFORMW %CALLNAME:LOCAL%は[妊娠]した
		TALENT:LOCAL:妊娠 = 1
		CALL CHANGE_N_STATUS(LOCAL)
	ENDIF
	CALL CLEAR_FLAG(LOCAL)
NEXT
FOR LOCAL,0,CHARANUM
	SIF GETBIT(TALENT:LOCAL:2,1) && MAXBASE:LOCAL:精力 <= 0
		MAXBASE:LOCAL:精力 = 10000
	CALL PREGNACY_S_BIRTH_CHECK(LOCAL)
	CALL PREGNACY_S_CHILD_CHECK(LOCAL)
	CALL PREGNACY_S_EVENT_FLAGCLEAR(LOCAL)
NEXT
RETURN 0

;-------------------------------------------------
;妊娠時のステータスの変化
;-------------------------------------------------
@CHANGE_N_STATUS(ARG)
;基本的に対象が変わるだけで同じなので関数にくくりだした
;体力上限を減らす
;体力が上限超えることもあるので、体力501以上なら500にする
MAXBASE:ARG:0 -= 500
IF BASE:ARG:0 >= 501
	BASE:ARG:0 = 500
ENDIF

;出産系のフラグ設定
CFLAG:ARG:72 = DAY + 20

;妊婦の胸の大きさをフラグに代入する処理
SIF CFLAG:ARG:77 == 0
	CFLAG:ARG:77 = TALENT:ARG:バストサイズ

;乳がワンランクパワーアップする
;絶→貧　貧→普　普→巨　巨・爆→そのまま
IF TALENT:ARG:バストサイズ <= 0
	PRINTFORMW %CALLNAME:ARG%の胸が大きくなった
	TALENT:ARG:バストサイズ += 1
	IF TALENT:ARG:105 == -1
		PRINTFORMW [絶壁]から[貧乳]ぐらいにはなった
	ELSEIF TALENT:ARG:105 == 0
		PRINTFORMW [貧乳]から普通のサイズになった
	ELSE
		PRINTFORMW [巨乳]になった
	ENDIF
	PRINTL 
ENDIF

;母乳出るようになる
IF TALENT:ARG:149 == 0
	PRINTFORMW %CALLNAME:ARG%は母乳が出るようになった
	PRINTL 
	TALENT:ARG:149 = 1
	MAXBASE:ARG:母乳 = 10000
	BASE:ARG:母乳 = 10000
ELSE
	;すでに母乳体質の場合はそのフラグを持っておく
	CFLAG:ARG:78 = 1
ENDIF
RETURN 0

;-------------------------------------------------
;臨月到達
;-------------------------------------------------
@PREGNACY_S_BIRTH_CHECK(ARG)
;妊娠していないとだめ
SIF CFLAG:ARG:にんっしんっ == 0
	RETURN 0
;出産日ならば出産
IF DAY >= CFLAG:ARG:72
	CALL PREGNACY_S_BIRTH(ARG)
	RETURN 1
ELSEIF ARG != MASTER && DAY == CFLAG:ARG:72 - 3
	PRINTFORML %CALLNAME:ARG%は臨月を迎えました
	PRINTFORMW %CALLNAME:ARG%は出産に備え永琳診療所へ移動します
	;部屋拡張に備えてとりあえず950
	CFLAG:ARG:現在位置 = 950
	SIF TARGET == ARG
		TARGET = 0
ELSEIF DAY == CFLAG:ARG:72 - 3
	PRINTFORML %CALLNAME:ARG%は臨月を迎えました
ENDIF

;-------------------------------------------------
;出産
;-------------------------------------------------
@PREGNACY_S_BIRTH(ARG)
;PRINTL 出産経験＋1
;EXP:ARG:52 += 1
PRINTW  

;誰の子か分からない場合は判明させる
SIF CFLAG:ARG:にんっしんっ > 1000
	CFLAG:ARG:にんっしんっ -= 1000

;生まれる人数設定
TRYCALL 生まれる人数, ARG
	
;子供を住人として追加するか里子に出すかチェック
LOCAL = -1
PRINTFORML %CALLNAME:ARG%の子供が産まれそうです。
PRINTFORML %CALLNAME:ARG%の子供を里子に出しますか？
PRINTFORML [0] - はい
PRINTFORML [1] - いいえ
$INPUT_LOOP
INPUT
IF RESULT == 0
	LOCAL = 0
ELSEIF RESULT == 1
	LOCAL = 1
ELSE
	GOTO INPUT_LOOP
ENDIF
IF LOCAL == 1
	;CFLAG:353の分だけ繰り返し
	FOR LOCAL, 0, CFLAG:ARG:353
		CALL PREGNACY_S_CHILD_BIRTH(ARG, LOCAL)
	NEXT
	;妊娠から育児へ移行
	TALENT:ARG:妊娠 = 0
	TALENT:ARG:育児中 = 1
	IF TALENT:ARG:母性 == 0
		PRINTFORMW %CALLNAME:ARG%は[母性]に目覚めた！
		TALENT:ARG:母性 = 1
		IF TALENT:ARG:40 >= 0
			PRINTFORML %CALLNAME:ARG%は産みの苦しみを経験したことで精神的に強くなった
			PRINTFORM %CALLNAME:ARG%は
			IF TALENT:ARG:40 == 1
				PRINTFORM [痛みに弱い]を克服し、
			ENDIF
			PRINTFORMW [痛みに強い]を得た
			TALENT:ARG:40 = -1
		ENDIF
	ELSE
		PRINTW  
	ENDIF
ELSE
	TRYCALL 里子, ARG
	PRINTFORML %CALLNAME:ARG%の子供は里子に出されました。
	;妊娠フラグ解除
	TALENT:ARG:妊娠 = 0
	;永琳診療所から私室へ戻す
	TRYCALL 私室戻しDEBUG, ARG
	CFLAG:ARG:現在位置 = GET_PRIVATE_ROOM(ARG, 0)
ENDIF


;出産表用の処理
;客
IF (CFLAG:ARG:にんっしんっ - 1) == 999
	親別出産数:ARG:299 += CFLAG:ARG:353
;訪問者
ELSEIF (CFLAG:ARG:にんっしんっ - 1) == 998
	親別出産数:ARG:298 += CFLAG:ARG:353
ELSE
	親別出産数:ARG:(CFLAG:ARG:にんっしんっ - 1) += CFLAG:ARG:353
ENDIF

CFLAG:ARG:にんっしんっ = 0


;妊娠時に減った体力の最大値が回復
MAXBASE:ARG:体力 += 500

;-------------------------------------------------
;妊娠パッチまわりのフラグをリセット
;-------------------------------------------------
@CLEAR_FLAG(ARG)
FOR LOCAL,0,CHARANUM
	CFLAG:LOCAL:71 = 0
	CFLAG:LOCAL:81 = 0
NEXT



;======================================================
;ここから先は子供に関する関数
;======================================================

;------------------------------------------------------
;誕生
;------------------------------------------------------
;ARG = 母親の番号, ARG:1 = 子供の番号 ARG:2 = 繰り返し番号
@PREGNACY_S_CHILD_BIRTH(ARG, ARG:2)
;息子か娘を半々で生成
IF RAND:2
	ADDCHARA 148
ELSE
	ADDCHARA 149
ENDIF
ARG:1 = CHARANUM - 1

;各種設定
;誕生日
CFLAG:(ARG:1):80 = DAY
;父
CFLAG:(ARG:1):父親 = CFLAG:ARG:にんっしんっ - 1
;母
CFLAG:(ARG:1):母親 = ARG
;成長度
CFLAG:(ARG:1):成長度 = 1
;私室設定
CFLAG:(ARG:1):311 = CFLAG:(CFLAG:(ARG:1):母親):311

;先天的な素質
IF CFLAG:(ARG:1):父親 < 900
	TALENT:(ARG:1):体型 = -2
	SIF TALENT:(CFLAG:(ARG:1):父親):体型 == -3 || TALENT:(CFLAG:(ARG:1):母親):体型 == -3
		TALENT:(ARG:1):体型 = -3
	SIF TALENT:(CFLAG:(ARG:1):父親):小悪魔 || TALENT:(CFLAG:(ARG:1):母親):小悪魔
		TALENT:(ARG:1):小悪魔 = 1
	SIF TALENT:(CFLAG:(ARG:1):父親):吸血鬼 || TALENT:(CFLAG:(ARG:1):母親):吸血鬼
		TALENT:(ARG:1):吸血鬼 = 1
	SIF TALENT:(CFLAG:(ARG:1):父親):動物耳 || TALENT:(CFLAG:(ARG:1):母親):動物耳
		TALENT:(ARG:1):動物耳 = 1
	LOCAL:1 = TALENT:(CFLAG:ARG:父親):薬毒耐性 + TALENT:(CFLAG:ARG:母親):薬毒耐性
	TALENT:ARG:薬毒耐性 = PREGNACY_S_CHILD_GET_TALENT_RANDOM(LOCAL:1)
	SIF TALENT:(CFLAG:(ARG:1):父親):猫舌 || TALENT:(CFLAG:(ARG:1):母親):猫舌
		TALENT:(ARG:1):猫舌 = 1
	SIF TALENT:(CFLAG:(ARG:1):父親):狐 || TALENT:(CFLAG:(ARG:1):母親):狐
		TALENT:(ARG:1):狐 = 1
	SIF TALENT:(CFLAG:(ARG:1):父親):妖狐 || TALENT:(CFLAG:(ARG:1):母親):妖狐
		TALENT:(ARG:1):妖狐 = 1
ELSE
	TALENT:(ARG:1):体型 = -2
	SIF TALENT:(CFLAG:(ARG:1):母親):体型 == -3
		TALENT:(ARG:1):体型 = -3
	SIF TALENT:(CFLAG:(ARG:1):母親):小悪魔
		TALENT:(ARG:1):小悪魔 = 1
	SIF TALENT:(CFLAG:(ARG:1):母親):吸血鬼
		TALENT:(ARG:1):吸血鬼 = 1
	SIF TALENT:(CFLAG:(ARG:1):母親):動物耳
		TALENT:(ARG:1):動物耳 = 1
	LOCAL:1 = TALENT:(CFLAG:ARG:母親):薬毒耐性
	TALENT:ARG:薬毒耐性 = PREGNACY_S_CHILD_GET_TALENT_RANDOM(LOCAL:1)
	SIF TALENT:(CFLAG:(ARG:1):母親):猫舌
		TALENT:(ARG:1):猫舌 = 1
	SIF TALENT:(CFLAG:(ARG:1):母親):狐
		TALENT:(ARG:1):狐 = 1
	SIF TALENT:(CFLAG:(ARG:1):母親):妖狐
		TALENT:(ARG:1):妖狐 = 1
ENDIF

CALL 親子関係追加取得, ARG:1, CFLAG:(ARG:1):父親, CFLAG:(ARG:1):母親
CALL 子供相性設定, ARG:1

SIF CFLAG:ARG:353 >= 2
	CALL 多生児フラグ処理, ARG:1, ARG:2
	
;一人目のときだけ口上表示
IF ARG:2 == 0
	IF CFLAG:(ARG:1):父親 == MASTER
		PRINTFORMW 鈴仙が突然%CALLNAME:MASTER%の元へと駆け込んできた
		PRINTFORMW どうやら%CALLNAME:ARG%が%CALLNAME:MASTER%の子を無事産んだらしい
		PRINTFORM %CALLNAME:MASTER%が急いで%CALLNAME:ARG%の元へ駆けつけると、
		IF TALENT:ARG:恋慕
			PRINTW 愛情に満ちた笑顔で迎えてくれた
		ELSE
			PRINTW 少し複雑な表情で迎えた
		ENDIF
		PRINTFORM %CALLNAME:ARG%が産んだのは元気な
		IF CFLAG:ARG:353 >= 2
			TRYCALL 多生児出産口上, CFLAG:ARG:353, 0
		ELSE
			IF GETBIT(TALENT:(ARG:1):2,0)
				PRINTW 女の子だ
			ELSEIF GETBIT(TALENT:(ARG:1):2,1)
				PRINTW 男の子だ
			ENDIF
		ENDIF
	ELSEIF CFLAG:(ARG:1):母親 == MASTER
		PRINTFORMW %CALLNAME:MASTER%のお腹が急に痛み出した
		PRINTW メイド妖精達に永琳診療所まで運んでもらい、診察してもらったところ、どうやら陣痛のようだ
		PRINTW そのまま出産まで永琳と鈴仙に診てもらった
		PRINTW 　　　　　　・
		PRINTW 　　　　　　・
		PRINTW 　　　　　　・
		PRINTFORM しばらくして漸く%CALLNAME:MASTER%は元気な
		IF CFLAG:ARG:353 >= 2
			TRYCALL 多生児出産口上, CFLAG:ARG:353, 1
		ELSE
			IF GETBIT(TALENT:(ARG:1):2,0)
				PRINT 女の子
			ELSEIF GETBIT(TALENT:(ARG:1):2,1)
				PRINT 男の子
			ENDIF
		ENDIF
		PRINTW を産んだ
		;訪問者とかの処理がいい加減めんｄ（ｒｙ
		IF CFLAG:(ARG:1):父親 < 900
			PRINTFORM 鈴仙に頼んで%CALLNAME:(CFLAG:(ARG:1):父親)%を呼んで来てもらうと、%CALLNAME:(CFLAG:(ARG:1):父親)%は
			IF TALENT:(CFLAG:(ARG:1):父親):恋慕
				PRINT 愛情に満ちた笑顔で
			ELSE
				PRINT 少し複雑な表情で
			ENDIF
			PRINTW 赤ちゃんを抱いた
		ELSE
			PRINTFORM %CALLNAME:MASTER%はこの
			IF CFLAG:(ARG:1):父親 == 998
				PRINT 訪問者
			ELSE
				PRINT 誰か
			ENDIF
			PRINT の子供を
			IF CFLAG:(ARG:1):父親 == 998 && CFLAG:MASTER:屈服度 >= 1000
				PRINT 大事に育てていこうと
			ELSE
				PRINT お腹を痛めて産んだのだからと自分に言い聞かせ、育てていく事を
			ENDIF
			PRINTW 心に決めた
		ENDIF
	;MASTERが両親ではないときの口上
	ELSE
		TRYCALL 両親がMASTER以外の出産口上, ARG, ARG:1
	ENDIF
ENDIF

;名前
IF CFLAG:(ARG:1):父親 == MASTER
	LOCAL = ARG
ELSEIF CFLAG:(ARG:1):母親 == MASTER
	LOCAL = CFLAG:(ARG:1):父親
ENDIF
IF CFLAG:(ARG:1):父親 == MASTER || CFLAG:(ARG:1):母親 == MASTER
	;双子以上の場合の分岐
	IF CFLAG:ARG:353 > 1
		PRINTFORML {ARG:2+1}人目の子供(\@ GETBIT(TALENT:(ARG:1):2,0) ?♀#♂\@)の名前を%N_T_R(LOCAL)%に付けてもらいますか？
	ELSE
		PRINTFORML %N_T_R(LOCAL)%に名前を付けてもらいますか？
	ENDIF
	PRINTL [0] 和風の名前で
	PRINTL [1] 洋風の名前で
	PRINTL [2] 任せます
	PRINTL [3] 自分で付けます
	$INPUT_LOOP_CHILD_NAME
	INPUT
	IF 0 <= RESULT && RESULT <= 2
		CALL PREGNACY_S_CHILD_NAME(LOCAL,ARG:1,RESULT)
		SELECTCASE LOCAL
			CASE 998
				PRINTFORMW 訪問者は%CALLNAME:(ARG:1)%と名付けました
			CASE 999
				PRINTFORMW 誰かは%CALLNAME:(ARG:1)%と名付けました
			CASEELSE
				PRINTFORMW %CALLNAME:LOCAL%は%CALLNAME:(ARG:1)%と名付けました
		ENDSELECT
	ELSEIF RESULT == 3
		PRINTL 名前を入力してください
		$INPUTS_LOOP_CHILD_NAMING
		INPUTS
		SIF RESULTS == ""
			GOTO INPUTS_LOOP_CHILD_NAMING
		PRINTFORM %CALLNAME:MASTER%は%RESULTS%と名付けました
		NAME:(ARG:1) = %RESULTS%
		CALLNAME:(ARG:1) = %RESULTS%
	ELSE
		GOTO INPUT_LOOP_CHILD_NAME
	ENDIF
ELSE
	CALL PREGNACY_S_CHILD_NAME(ARG,ARG:1,2)
ENDIF

CALL 子宮内体積設定(ARG:1)

;永琳診療所から私室へ戻す
IF (ARG:2 + 1) == CFLAG:ARG:353
	TRYCALL 私室戻しDEBUG, ARG
	IF ARG != MASTER
		IF ARG == 1
			CFLAG:ARG:現在位置 = 63
		ELSEIF ARG == 2
			CFLAG:ARG:現在位置 = 53
		ELSEIF ARG == 3
			CFLAG:ARG:現在位置 = 43
		ELSEIF ARG == 4
			CFLAG:ARG:現在位置 = 33
		ELSEIF ARG == 5
			CFLAG:ARG:現在位置 = 13
		ELSEIF ARG == 6
			CFLAG:ARG:現在位置 = 11
		ELSEIF ARG == 7
			CFLAG:ARG:現在位置 = 10
		ELSEIF ARG == 8
			CFLAG:ARG:現在位置 = 12
		;その他追加されたキャラ用(息子・娘)
		ELSE
			CFLAG:ARG:現在位置 = CFLAG:ARG:311
		ENDIF
	ENDIF
ENDIF

;------------------------------------------------------
;成長
;------------------------------------------------------
@PREGNACY_S_CHILD_CHECK(ARG)
IF DAY > CFLAG:ARG:80 + 20 && CFLAG:ARG:成長度 == 1
	TALENT:ARG:幼稚 = 0
	IF CFLAG:ARG:父親 < 900
		SIF TALENT:(CFLAG:ARG:父親):幼稚 && TALENT:(CFLAG:ARG:母親):幼稚
			TALENT:ARG:幼稚 = 1
	ELSEIF TALENT:(CFLAG:ARG:母親):幼稚
		TALENT:ARG:幼稚 = 1
	ENDIF
	CFLAG:ARG:成長度 = 2
	IF CFLAG:ARG:父親 < 900
		MAXBASE:ARG:体力 = (MAXBASE:(CFLAG:ARG:父親):体力 + MAXBASE:(CFLAG:ARG:母親):体力) / 2 / 2
		MAXBASE:ARG:気力 = (MAXBASE:(CFLAG:ARG:父親):気力 + MAXBASE:(CFLAG:ARG:母親):気力) / 2 / 2
	ELSE
		MAXBASE:ARG:体力 = (MAXBASE:(CFLAG:ARG:母親):体力) / 2
		MAXBASE:ARG:気力 = (MAXBASE:(CFLAG:ARG:母親):気力) / 2
	ENDIF
	PRINTFORMW %CALLNAME:ARG%は%CALLNAME:(CFLAG:ARG:母親)%の元を離れ、一人で出歩けるようになった

	;母親の育児中の人数減少
	CFLAG:(CFLAG:ARG:母親):353 --
	
	;育児中の人数が0になれば素質[育児中]が消滅
	SIF CFLAG:(CFLAG:ARG:母親):353 == 0
		TALENT:(CFLAG:ARG:母親):育児中 = 0
	;[妊娠]未取得且つ育児中の人数が0になれば胸が元に戻る
	IF CFLAG:(CFLAG:ARG:母親):353 == 0 && !TALENT:(CFLAG:ARG:母親):妊娠
		PRINTFORML %CALLNAME:(CFLAG:ARG:母親)%の胸は母乳を与える役割を終えました
		IF TALENT:(CFLAG:ARG:母親):バストサイズ != CFLAG:(CFLAG:ARG:母親):77
			PRINTFORMW 子供に母乳を与えたことで%CALLNAME:(CFLAG:ARG:母親)%の胸が小さくなりました
			;妊娠時に大きくなった胸のサイズを一段小さくする
			TALENT:(CFLAG:ARG:母親):バストサイズ = CFLAG:(CFLAG:ARG:母親):77
		ELSE
			PRINTW  
		ENDIF
		;妊娠時に発生した母乳体質を消失させる
		IF !CFLAG:(CFLAG:ARG:母親):78
			TALENT:(CFLAG:ARG:母親):149 = 0
			MAXBASE:(CFLAG:ARG:母親):3 = 0
		ENDIF
