	LOCAL = PREGNACY_S_CHILD_GET_TALENT_RANDOM(LOCAL:1)
	SIF LOCAL > TALENT:ARG:体型
		TALENT:ARG:体型 = LOCAL
	IF GETBIT(TALENT:ARG:2,0)
		;母親が並乳以上ならまだチャンスが！
		IF TALENT:(CFLAG:ARG:母親):バストサイズ >= 0
			SIF RAND:(2 + EXP:ARG:Ｂ経験 / 10)
