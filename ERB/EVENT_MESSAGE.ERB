	IF TFLAG:25 == 1 && M:1 == 0
		PRINTL そして従順がLV2になった
		M:1 = 1
	ENDIF
ENDIF


@MARK_MESSAGE_R
;反発刻印
IF TFLAG:21 == 1
	PRINTFORML %CALLNAME:TARGET%は鋭い視線をこちらに向けている……
	PRINTFORML %CALLNAME:TARGET%は反発刻印LV1を得た
ELSEIF TFLAG:21 == 2
	PRINTFORML %CALLNAME:TARGET%は怒りを滲ませた表情をしている……
	PRINTFORML %CALLNAME:TARGET%は反発刻印LV2を得た
	IF TFLAG:25 == 1
		PRINTL そして従順がLV0に下がった
		M:1 = 1
	ELSEIF TFLAG:25 == 2 && M:1 == 0
		PRINTL そして従順がLV1に下がった
