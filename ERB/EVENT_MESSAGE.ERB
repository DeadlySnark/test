@MARK_MESSAGE_A
;苦痛刻印
IF TFLAG:22 == 1
	PRINTFORML %CALLNAME:TARGET%は顔を歪ませながら苦痛を堪えている……
	PRINTFORML %CALLNAME:TARGET%は苦痛刻印LV1を得た
ELSEIF TFLAG:22 == 2
	PRINTFORML %CALLNAME:TARGET%はしばらくの間、苦鳴を漏らし続けていた……
	PRINTFORML %CALLNAME:TARGET%は苦痛刻印LV2を得た
	IF TFLAG:25 == 1 && M:1 == 0
		PRINTL そして従順がLV1になった
		M:1 = 1
	ENDIF
ELSEIF TFLAG:22 == 3
	PRINTFORML %CALLNAME:TARGET%は余りの苦痛に泣き喚いていた……
	PRINTFORML %CALLNAME:TARGET%は苦痛刻印LV3を得た
	IF TFLAG:25 == 1 && M:1 == 0
		PRINTL そして従順がLV1になった
		M:1 = 1
	ENDIF
	IF TALENT:PLAYER:サド
		PRINTFORML 【%CALLNAME:PLAYER%がサド】
		PRINTL 異常経験＋２
	ELSEIF TALENT:PLAYER:針さばき && SELECTCOM == 104
		PRINTFORML 【%CALLNAME:PLAYER%が針さばき】
		PRINTL 異常経験＋２
	ELSE
		PRINTL 異常経験＋１
	ENDIF
ENDIF


@MARK_MESSAGE_P
;快楽刻印
