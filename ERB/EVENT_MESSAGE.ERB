	ENDIF
ELSEIF TFLAG:21 == 3
	PRINTFORML %CALLNAME:TARGET%は憤怒に顔を歪ませて、恨み言を呟いている……
	PRINTFORML %CALLNAME:TARGET%は反発刻印LV3を得た
	IF TFLAG:25 == 1 && M:1 == 0
		PRINTL そして従順がLV0に下がった
		M:1 = 1
	ELSEIF TFLAG:25 == 2 && M:1 == 0
		PRINTL そして従順がLV2に下がった
		M:1 = 1
	ENDIF
ENDIF

