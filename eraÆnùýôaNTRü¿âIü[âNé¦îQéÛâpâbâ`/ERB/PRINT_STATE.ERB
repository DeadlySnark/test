			IF ARG:1 == -2
				RETURNF "絶壁"
			ELSEIF ARG:1 == -1
				RETURNF "貧乳"
			ELSEIF ARG:1 == 1
				RETURNF "巨乳"
			ELSEIF ARG:1 == 2
